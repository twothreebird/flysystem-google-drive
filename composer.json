{
    "name": "nao-pon/flysystem-google-drive",
    "description": "Flysystem adapter for Google Drive",
    "license": "MIT",
    "authors": [
        {
            "name": "Naoki Sawada",
            "email": "hypweb@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "league/flysystem": "dev-feature/laravel-upgrade",
        "google/apiclient": "^2.0",
        "nao-pon/flysystem-cached-extra": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.5.1"
    },
    "autoload": {
        "psr-4": {
            "Hypweb\\Flysystem\\GoogleDrive\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-api_v2": "1.0.x-dev",
            "dev-master": "1.1.x-dev"
        }
    },
    "repositories": {
        "twothreebird": {
            "type": "composer",
            "url": "https://twothreebird.repo.repman.io"
        }
    }
}
